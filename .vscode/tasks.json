{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/work_webapp/work_webapp.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "publish",
        "${workspaceFolder}/work_webapp/work_webapp.csproj",
        "-c",
        "Debug",
        "-o",
        "${workspaceFolder}/work_webapp/bin/Debug/net9.0/publish"
      ]
    }
  ]
}
